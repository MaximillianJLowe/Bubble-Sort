var b = [8, 4, 4, 9483, 2, 105, 55];
function bubbleSort(b)
{
    var swapped;
    do {
        swapped = false;
        for (var i=0; i < b.length-1; i++) {
            if (b[i] > b[i+1]) {
                var temp = b[i];
                b[i] = b[i+1];
                b[i+1] = temp;
                swapped = true;
            }
        }
    } while (swapped);
}

bubbleSort(b);
console.log(b);
